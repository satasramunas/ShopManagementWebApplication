@model List<ShopItem>

<h3>Testing Shop</h3>
<table>


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Item</th>
      <th scope="col">Shop Name</th>
      <th scope="col">Expiry Date</th>
      <th scope="col">Actions</th>
      
    </tr>
  </thead>
  <tbody>
      @foreach (var item in @Model)
        {
        <tr>
          <th scope="row"></th>
          <td>@item.Name</td>
          <td>@item.ShopName</td>
          <td>@item.ExpiryDate</td>
          <td>@Html.ActionLink("Remove", "Delete", "Shop", new { Name = @item.Name}, new { @class = "btn btn-primary"})</td>
          <td>@Html.ActionLink("Edit", "Update", "Shop", new { Name = @item.Name}, new { @class = "btn btn-primary"})</td>
        </tr>
        }
    
    
  </tbody>
</table>